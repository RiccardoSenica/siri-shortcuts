{
  "env": {
    "node": true,
    "es2021": true,
    "jest": true
  },
  "extends": ["next/core-web-vitals", "eslint:recommended", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "@typescript-eslint/no-unused-vars": "error",
    "no-console": "off"
  }
}
